    "keywords": [
        "laravel-enso",
        "datatable",
        "data-table",
        "datatables.net",
        "data-table-server-side"
    ],
    "homepage": "https://github.com/laravel-enso/DataTable",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Adrian Ocneanu",
            "email": "aocneanu@gmail.com",
            "homepage": "https://laravel-enso.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.1.0",
        "laravel/framework": "5.4.*",
        "laravel-enso/helpers": "1.2.*",
        "maatwebsite/excel": "^2.1"
    },
    "autoload": {
        "psr-4": {
            "LaravelEnso\\DataTable\\": "src/"
        }
